// modified from https://github.com/beneater/eeprom-programmer/blob/master/eeprom-programmer/eeprom-programmer.ino

// This works only with my hardware config , change the constants below for your programmer

#define SHIFT_CLK 3
#define SHIFT_DATA 4
#define SHIFT_LATCH 2
#define EEPROM_D0 5
#define EEPROM_D7 12
#define WRITE_EN 13

void setAddress( int address ) {
  shiftOut(SHIFT_DATA, SHIFT_CLK, MSBFIRST, (address >> 8));
  shiftOut(SHIFT_DATA, SHIFT_CLK, MSBFIRST, address);

  digitalWrite(SHIFT_LATCH, LOW);
  digitalWrite(SHIFT_LATCH, HIGH);
  digitalWrite(SHIFT_LATCH, LOW);
}

void writeEEPROM(int address, byte data){
  setAddress(address);
  for (int pin = EEPROM_D0; pin <= EEPROM_D7; pin ++){
    pinMode(pin, OUTPUT);
  }

  for (int pin = EEPROM_D0; pin <= EEPROM_D7; pin ++){
    digitalWrite(pin, data & 1);
    data = data >> 1;
  }

  digitalWrite(WRITE_EN, LOW);
  delayMicroseconds(5);
  digitalWrite(WRITE_EN, HIGH);
  delay(20);

}
